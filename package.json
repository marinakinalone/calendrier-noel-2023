{
  "name": "calendrier-noel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . && bun run typecheck",
    "lint:staged": "eslint $(git diff --cached --name-only --diff-filter=ACM \"*.ts\" \"*.tsx\" | sed 's| |\\\\ |g')",
    "prettier": "prettier --write **/*.{ts,tsx}",
    "prettier:staged": "prettier --write $(git diff --cached --name-only --diff-filter=ACM \"*.ts\" \"*.tsx\" | sed 's| |\\\\ |g')",
    "typecheck": "tsc"
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "^18",
    "react-dom": "^18",
    "typecheck": "^0.1.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "classnames": "^2.3.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5"
  }
}
